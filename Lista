Questão 12
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        //linhas/colunas
        string [,] jogoDeValha= {
                              {"x","o","_"},
                              {"o","x","x"},
                              {"_","o","o"}};
                              Console.WriteLine("o jogo da velha atual é:");
        for(int i=0; i < 3; i++)
        {
            
            for(int j=0; j < 3; j++)
            {
               if (j == 2)
                {
                    Console.WriteLine(jogoDeValha[i,j]+" ");
                   
                }
                else
                {
                     Console.Write(jogoDeValha[i,j]+" "); 
                }
                
                
                
                    
               
            }
        }
        Console.WriteLine("para que o jogador com o (o) ganhe a partida ele deve:");
            for(int i=0; i < 3; i++)
        {
            
            for(int j=0; j < 3; j++)
            {
                if (j == 0)
                    {
                        jogoDeValha[i,j]="o";
                    }
               if (j == 2)
                {
                    Console.WriteLine(jogoDeValha[i,j]+" ");
                   
                }
                else
                {
                     Console.Write(jogoDeValha[i,j]+" "); 
                }
    
            }
        }
        
    }
}
