Questão 10
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        double saldo=0;
        int anoAtual=1;
        bool continuar=true;

        Console.Write("Quanto será investido por mês? ");
        double investimentoMensal = double.Parse(Console.ReadLine());

        Console.Write("Qual será a taxa de juros mensal ? ");
        double taxaMensal = double.Parse(Console.ReadLine());
         taxaMensal=taxaMensal/100;
        while (continuar)
        {
            
            for (int mes = 1; mes <= 12; mes++)
            {
              
                saldo = saldo + investimentoMensal;
                
                
                saldo = saldo + (saldo * taxaMensal);
            }

            Console.WriteLine($"Saldo do investimento após {anoAtual} ano(s): {saldo}");

            Console.Write("Deseja processar mais um ano? (S/N): ");
            string resposta = Console.ReadLine().ToUpper();

            if (resposta == "S")
            {
                anoAtual++;
            }
            else
            {
                continuar = false;
            }
        }
    }
}

