Questão 18
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        
        
        Console.WriteLine("informe um valor para ser traduzido em extenso");
        int entrada =int.Parse(Console.ReadLine());
        int milhoes = entrada / 1000000;
        int milhares = (entrada % 1000000) / 1000;
        int unidades = entrada % 1000;
        string resultado=" ";
        if (milhoes > 0)
        //para adicionar os milões
        {

            resultado += traduzir(milhoes);
            if (milhoes == 1)
            {
                resultado += "milhão";
            }
            else
            {
                resultado += "milhões";
            }
            
        }
        if (milhares > 0)
        //para adicionar os milhares
        {

          if(resultado!=" ")
            {
                resultado += ",";
            };
            if (milhares == 1)
            {
                resultado += " mil ";
            }
            else
            {
                resultado += traduzir(milhares)+ " mil";
            }
            
        }
        if (unidades > 0)
        //para adicionar as unidades
        {

          if(resultado!=" ")
            {
                resultado += " e ";
                resultado += traduzir(unidades);
            };
            
        }
        
        
         Console.WriteLine(resultado);
    }
    static int ObterCentena(int n) {
    return (n % 1000) / 100;
}

static int ObterDezena(int n) {
    
    return (n % 100) / 10;
}

static int ObterUnidade(int n) {
    return n%10;
    
}
static string traduzir(int n)
    {
        if (n == 100) return "cem";
        if (n == 0) return "zero";
        string[] unidade_ = { "", "um", "dois", "três", "quatro", "cinco", "seis", "sete", "oito", "nove", "dez", "onze", "doze", "treze", "quatorze", "quinze", "dezesseis", "dezessete", "dezoito", "dezenove" };
        string[] dezena_ = { "", "", "vinte", "trinta", "quarenta", "cinquenta", "sessenta", "setenta", "oitenta", "noventa" };
        string[] centena_ = { "", "cento", "duzentos", "trezentos", "quatrocentos", "quinhentos", "seiscentos", "setecentos", "oitocentos", "novecentos" }; 
        int centena=ObterCentena(n);
         int dezena=ObterDezena(n);
        int unidade =ObterUnidade(n);
        //adicionar a centena primerio para manter a sintaxe correta
        string res=centena_[centena];
        // para pegar as dezenas e unidades
        int DezenaUnidade=n%100;
        //checar se o  numero pracisa adiconar um 'e'
        if ((centena > 0) && (DezenaUnidade>0))
        {
            res += " e ";
        }
        if ((DezenaUnidade >= 10) && (DezenaUnidade<=19))
        {
            res=unidade_[DezenaUnidade];
        }
        else
        {
            res += dezena_[dezena];
            if (dezena > 0 && unidade > 0)
            {
             res += " e ";

            }
            
        }
        res += 
