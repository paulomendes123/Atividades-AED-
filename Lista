Questão 4
public class formulaDeBaskara
{
  public static void Main(string[] args)
{
  Produto a = new Produto();
        Console.WriteLine("insira o nome do produto");
       a.nome_=Console.ReadLine();
       Console.WriteLine("insira o o preço do produto");
       a.preço_=float.Parse(Console.ReadLine());
       Console.WriteLine("insira a quantidade de produtos");
       a.quantidade_=int.Parse(Console.ReadLine());
    if ((a.quantidade_ > 10)&&(a.quantidade_<=20))
    {
      a.preço_=a.preço_*0.9f;
    }
    if ((a.quantidade_ > 20)&&(a.quantidade_<=50))
    {
      a.preço_=a.preço_*0.8f;
    }
     if (a.quantidade_ > 50)
    {
      a.preço_=a.preço_*0.75f;
    }
        Console.WriteLine("PRODUTO:{0}",a.nome_);
        Console.WriteLine("A QUANTIDADE TOTAL SERA: {0}",a.quantidade_);
        Console.WriteLine("PREÇO TOTAL SERA: {0}",a.preço_);
}
public class Produto
  {
    private float preço;
    public float preço_ {get; set;}
    private string nome;
    public string nome_ {get; set;}
    private int quantidade;
    public int quantidade_ {get; set;}

    public Produto()
    {
      
    }
    public Produto (float preço, string nome, int quantidade) {
      this.preço=preço;
      this.nome=nome;
      this.quantidade=quantidade;
    }


  }
}


