Questão 11
public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        double entrada;
        int i=0;
        Console.WriteLine("insira um valor para encontrar a raiz aproximada");
        entrada=int.Parse(Console.ReadLine());
    ;
    //encontrando as raizes proximas
        while ((i * i) <= entrada)
            {
                i++;
            }
            //definindo a raiz porterior e anterior
            double raizAcima = i;
            double raizAbaixo = i - 1;
            double quadradoAbaixo = raizAbaixo * raizAbaixo;
            double quadradoAcima = raizAcima * raizAcima;
            double guarda=raizAbaixo;
            //checando os quadrados proximos
           
                 while ((raizAbaixo + 0.1 )*(raizAbaixo + 0.1 ) <= entrada)
            {
                raizAbaixo= raizAbaixo + 0.1;
            }
                
            entrada=raizAbaixo;
            Console.WriteLine($"o quadrado mais proximo abaixo é {quadradoAbaixo} e acima é {quadradoAcima} as raizes são respectivamente {guarda} ; {raizAcima} e o resultado aproximado é {entrada}");
            
              
      
    }
}
