Questão 2:
public class formulaDeBaskara
{
  public static void Main(string[] args)
{
        float x1;
        float y1;
        float z1;
        float x2;
        float y2;
        float z2; 
        double d;
        Console.WriteLine("insira o valor do primeiro ponto do eixo x");
        x1=float.Parse(Console.ReadLine());
        Console.WriteLine("insira o valor do primeiro ponto do eixo y");
        y1=float.Parse(Console.ReadLine());
        Console.WriteLine("insira o valor do primeiro ponto do eixo z");
        z1=float.Parse(Console.ReadLine());
        Console.WriteLine("insira o valor do segundo ponto do eixo x");
        x2=float.Parse(Console.ReadLine());
        Console.WriteLine("insira o valor do segundo ponto do eixo y");
        y2=float.Parse(Console.ReadLine());
        Console.WriteLine("insira o valor do segundo ponto do eixo z");
         z2=float.Parse(Console.ReadLine());
          float dx = x1-x2;
        float dy=y1-y2;
        float dz=z1-z2;
        d= Math.Sqrt((dx*dx)+(dy*dy)+(dz*dz));
        Console.WriteLine("a distancia entre os dois pontos é: {0}",d);
        
}
}

