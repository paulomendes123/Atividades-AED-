Questão 13
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        Random gerador = new Random();
        int[] ordenado= new int [100];
        for(int i=0; i < ordenado.Length; i++)
        {
            ordenado[i] = gerador.Next(0, 100);
        }
       
        for(int j=0; j < ordenado.Length - 1; j++)
        {
            for(int i=0; i < ordenado.Length - 1; i++)
            {
                if (ordenado[i] > ordenado[i+1]){
                int guarda=ordenado[i];
                ordenado[i]=ordenado[i+1];
                ordenado[i+1]=guarda;
                }
            }
        }
        for(int i=0; i < ordenado.Length; i++)
            {
                Console.WriteLine(ordenado[i]);
            }
    }
}
