Questão 1:
public class formulaDeBaskara
{
  public static void Main(string[] args)
{
   int a=0;
   int b;
   int c;
   double xmenos;
   double xmais;
   float delta;
   float raiz;
   while(a==0){
   Console.WriteLine("insira o A equação");
   a=int.Parse(Console.ReadLine());
        if (a == 0)
        {
          Console.WriteLine("0 nao he uma entrada valida");  
          
        }
   }
   Console.WriteLine("insira o B equação");
   b=int.Parse(Console.ReadLine());
   Console.WriteLine("insira o C equação");
   c=int.Parse(Console.ReadLine());

  delta= (b*b) - 4*a*c;
        if (delta < 0)
        {
            Console.WriteLine("nao existem raizes reais para delta 0");
        }
        else
        {
            xmais=(-b + Math.Sqrt(delta)) / (2 * a);
 xmenos=(-b - Math.Sqrt(delta)) / (2 * a);
 Console.WriteLine($"o resultado é x1: {xmais} / x2: {xmenos}");
        }
}
}

