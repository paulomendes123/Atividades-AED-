Questão 8
public class formulaDeBaskara
{
  public static void Main(string[] args)
{
  int entrada;
  Console.WriteLine("isnira um numero para a sequencia de fibonaci");
  entrada=int.Parse(Console.ReadLine());
  entrada=entrada+1;
  int[] fibonaci = new int[entrada];
for(int i=0; i < entrada; i++)
    {
      if (i == 0)
      {
        fibonaci[i]=0;
        Console.WriteLine(fibonaci[i]);
        continue;
      }
      if (i == 1)
      {
        fibonaci[i]=1;
        Console.WriteLine(fibonaci[i]);
        continue;
        
      }
      fibonaci[i]=fibonaci[i-1]+fibonaci[i-2];
      Console.WriteLine(fibonaci[i]);

    }   
    
}

}
