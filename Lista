Questão 16
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        
        int segundos= Ler();   
        int horas=calcularHora(segundos);
        int minutos=CALCUALRMIN(segundos);
        int totalseg=restseg(segundos);
        escrita(horas, minutos, totalseg);
;
    }
    static int calcularHora(int a)
    {
        int h=a/3600;
        return h;
    }
    static int Ler()
    {
        Console.WriteLine("Insira os segundos");
        return int.Parse(Console.ReadLine());
    }
    static int CALCUALRMIN(int a)
    {
        int min=(a% 3600) / 60;
       return min;
    }
    static void escrita (int a, int b, int c)
    {
         Console.WriteLine($"esses segundos representam um total de {a} horas, {b} minutos e {c} segundos ");
    }
    static int restseg(int a)
    {
        return a%60;
    }
    
}
