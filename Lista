17
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        
        
        Console.WriteLine("informe um valor para ser traduzido em extenso");
        int entrada =int.Parse(Console.ReadLine());
         int centena=ObterCentena(entrada);
         int dezena=ObterDezena(entrada);
        int unidade =ObterUnidade(entrada);
        
         //string extenso=traduzir(centena, dezena, unidade, entrada);
         Console.WriteLine(traduzir(centena, dezena,unidade, entrada));
    }
    static int ObterCentena(int n) {
    return (n % 1000) / 100;
}

static int ObterDezena(int n) {
    
    return (n % 100) / 10;
}

static int ObterUnidade(int n) {
    return n%10;
    
}
static string traduzir(int a, int b, int c, int n)
    {
        if (n == 100) return "cem";
        if (n == 0) return "zero";
        string[] unidade_ = { "", "um", "dois", "três", "quatro", "cinco", "seis", "sete", "oito", "nove", "dez", "onze", "doze", "treze", "quatorze", "quinze", "dezesseis", "dezessete", "dezoito", "dezenove" };
        string[] dezena_ = { "", "", "vinte", "trinta", "quarenta", "cinquenta", "sessenta", "setenta", "oitenta", "noventa" };
        string[] centena_ = { "", "cento", "duzentos", "trezentos", "quatrocentos", "quinhentos", "seiscentos", "setecentos", "oitocentos", "novecentos" }; 
        //adicionar a centena primerio para manter a sintaxe correta
        string resultado=centena_[a];
        // para pegar as dezenas e unidades
        int DezenaUnidade=n%100;
        //checar se o  numero pracisa adiconar um 'e'
        if ((a > 0) && (DezenaUnidade>0))
        {
            resultado += " e ";
        }
        if ((DezenaUnidade >= 10) && (DezenaUnidade<=19))
        {
            resultado=unidade_[DezenaUnidade];
        }
        else
        {
            resultado += dezena_[b];
            if (b > 0 && c > 0)
            {
             resultado += " e ";

            }
            
        }
        resultado += unidade_[c];

        return resultado;
    }
    
}
