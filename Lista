using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        Random gerador = new Random();
        int[] ordenado1= new int [50];
        int[] ordenado2= new int [50];
        int[] ordenado3= new int [100];
         int guarda;
        for(int i=0; i < ordenado1.Length; i++)
        {
            ordenado1[i] = gerador.Next(0, 100);
        }
       
        for(int j=0; j < ordenado1.Length - 1; j++)
        {
            for(int i=0; i < ordenado1.Length - 1; i++)
            {
                if (ordenado1[i] > ordenado1[i+1]){
                 guarda=ordenado1[i];
                ordenado1[i]=ordenado1[i+1];
                ordenado1[i+1]=guarda;
                }
            }
        }
        for(int i=0; i < ordenado1.Length; i++)
            {
                Console.WriteLine(ordenado1[i]);
            }
            ////////////////////////////////////
             Console.WriteLine("_------------------------------------------------------------------------------------------------------------------------------------");
            for(int i=0; i < ordenado2.Length; i++)
        {
            ordenado2[i] = gerador.Next(0, 100);
        }
       
        for(int j=0; j < ordenado2.Length - 1; j++)
        {
            for(int i=0; i < ordenado2.Length - 1; i++)
            {
                if (ordenado2[i] > ordenado2[i+1]){
                 guarda=ordenado2[i];
                ordenado2[i]=ordenado2[i+1];
                ordenado2[i+1]=guarda;
                }
            }
        }
        for(int i=0; i < ordenado2.Length; i++)
            {
                Console.WriteLine(ordenado2[i]);
            }
             ///////////////////////
             Console.WriteLine("_------------------------------------------------------------------------------------------------------------------------------------");
             for(int i=0; i < 50; i++)
        {
            
            ordenado3[i] = ordenado1[i];
            

        }
        for(int i=50; i < 100; i++)
        {
            
            ordenado3[i] = ordenado2[i];
            

        }
        for(int j=0; j < ordenado2.Length - 1; j++)
        {
            for(int i=0; i < ordenado2.Length - 1; i++)
            {
                if (ordenado3[i] > ordenado3[i+1]){
                 guarda=ordenado3[i];
                ordenado3[i]=ordenado3[i+1];
                ordenado3[i+1]=guarda;
            }
            }

        }
         for(int i=0; i < ordenado2.Length; i++)
            {
                Console.WriteLine(ordenado3[i]);
            }
    }
}
