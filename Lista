Questão 15
using System;

public class CalculadoraInvestimento
{
    public static void Main(string[] args)
    {
        Console.WriteLine("insira o nome do aluno");
        string nome = Console.ReadLine();
        Console.WriteLine("informe a nota n1 do aluno");
        float notan1 = float.Parse(Console.ReadLine());
        Console.WriteLine("informe a nota n2 do aluno");
        float notan2 = float.Parse(Console.ReadLine());
        float media= CalcularMedia(notan1 , notan2);
        statusDoAluno(media);
       
        
    }
    static float CalcularMedia(float a, float b)
    {
        return (a+b)/2;
    }
    static void statusDoAluno(float a)
    {
        if (a > 6)
        {
             Console.WriteLine("o status do aluno é: aprovado");
        }
        else if (a > 4)
        {
            Console.WriteLine("o status do aluno é: Verificação	Suplementar");
        }
        else
        {
            Console.WriteLine("o status do aluno é: reprovado");
        }
    }
}
