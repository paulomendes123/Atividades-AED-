Questão 1
using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        
        
    }
    public class veiculo
        {
            //quantidade de ocupantes
        private int quantocup;
        
         //quantidade de rodas
        private int quantrodas;
        
         //quantidade maxima de litros
        private int maxlitros;
        
         //quantidade de litros atuais
        private float quantlitros;
        
         //quantidade de consumo médio (em km/litro) de combustível 
        private float consumomedia;
        
        public float cm
        {
        get{return consumomedia;}
        set{ consumomedia = cm;}
             }
        
         //quilometragem atual do veículo
        private float kmatual;
        public float kma
        {
        get{
            return kmatual;
        }
        set
        { kmatual = kma;
            
        }
        }
         //Autonomia do veículo, que identifica a quantidade de Km que ele ainda pode percorrer sem abastecer
        private float autonomia;
       public class Carro:veiculo{
           public Carro(){
               }
           public Carro( int qportas, string modelo, int quantocup, int quantrodas, int maxlitros, float consumomedia, int kmatual){
               this.qportas=qportas;
               this.modelo=modelo;
               this.quantocup=quantocup;
               this.quantrodas=quantrodas;
               this.maxlitros=maxlitros;
               this.consumomedia=consumomedia;
               this.kmatual=kmatual;
           }
            public int qportas;
            public string modelo;
            public float perrcorrer(float qtdQuilometros){
                float nkm = kmatual+qtdQuilometros;
                if(nkm>999999){
                    return kmatual=999999;
                }
                else{
                return kmatual=nkm;
                }
            }
        }
        
        
}
}

